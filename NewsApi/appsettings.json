{
  "HackerNews": {
    "BaseUrl": "https://hacker-news.firebaseio.com/v0/",
    "MaxPageSize": 20,
    "CacheDurationMinutes": 5,
    "StoriesCacheKey": "cached-stories",
    "StoriesCacheDurationHours": 1,
    "ApiTimeout": "00:00:30",
    "RetryCount": 3,
    "RetryWaitSeconds": 2,
    "RateLimit": {
      "PermitLimit": 100,
      "WindowSeconds": 60
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "https://yourdomain.com"
    ]
  }
}
